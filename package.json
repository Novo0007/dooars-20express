    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.11",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-pwa": "^0.20.5",
    "workbox-window": "^7.3.0",
