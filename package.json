{
  "name": "fusion-vue-tailwind-starter",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "json-server": "json-server --watch db.json --port 3001 --host 0.0.0.0",
    "dev:full": "run-p dev json-server",
    "dev:api": "json-server --watch db.json --port 3001 --host 0.0.0.0"
  },
  "dependencies": {
    "vue": "^3.5.17",
    "pinia": "^2.2.6",
    "vue-router": "^4.5.0",
    "vue-i18n": "^10.0.7",
    "leaflet": "^1.9.4",
    "vue3-leaflet": "^1.0.16",
    "@vueuse/core": "^11.3.0",
    "@vueuse/head": "^2.0.0",
    "@supabase/supabase-js": "^2.48.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@types/leaflet": "^1.9.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
